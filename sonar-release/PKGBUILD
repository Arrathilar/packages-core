pkgname=sonar-release
pkgver=17.1dev
pkgrel=1
pkgdesc="Sonar's release definition"
arch=("any")
url="http://www.sonargnulinux.com"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
conflicts=('manjaro-release')
install="sonar-release.install"
sha256sums=('ed5fde81e8b5b0b0a605781b7d6d38f162e2755c7cd96364663c478e00ddf3b2')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/
}
