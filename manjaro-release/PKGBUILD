pkgname=manjaro-release
pkgver=15.09rc4
pkgrel=1
pkgdesc="Manjaro's release definition"
arch=("any")
url="http://manjarolinux.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('2bc1f610397c99189f25cbc2851c93f3e5e355f4fbee8aa3c2e0b1034a62a1f0')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}
