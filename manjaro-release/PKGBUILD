pkgname=manjaro-release
pkgver=17.1rc3
pkgrel=1
pkgdesc="Manjaro's release definition"
arch=("any")
url="http://manjarolinux.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('eaa50319db4474f5fda8ae4eeb2ff3a0a78e75cfa33f8a04144bb7e92dbbbb22')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}
