pkgname=manjaro-release
pkgver=15.09rc1
pkgrel=2
pkgdesc="Manjaro's release definition"
arch=("any")
url="http://manjarolinux.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
install="manjaro-release.install"
sha256sums=('28873b3008a644aeb2a8f9659027111b3dce5f3e5ab09f2e820ba7116134337f')

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}
